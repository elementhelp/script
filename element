local HttpService = game:GetService("HttpService")

-- ID-ul unic setat de bot (asta vine din loadstring-ul generat)
local ID = _G.ELEMENT_ID or "MISSING_ID"

-- API-ul tău proxy (Railway unde rulează mine.py)
local API_URL = "https://element.up.railway.app/api/config?id=" .. ID

-- Încercăm să luăm config-ul
local success, response = pcall(function()
    return HttpService:GetAsync(API_URL)
end)

if not success then
    warn("❌ Failed to fetch config from API:", response)
    return
end

local data = HttpService:JSONDecode(response)

if data.error then
    warn("❌ API Error:", data.error)
    return
end

-- ✅ Avem config-ul
print("✅ Element Config Loaded")
print("Username:", data.username or "Unknown")
print("Webhook:", data.webhook_url or "None")

-- 🔹 Aici poți adăuga ce vrei să facă scriptul tău după ce ia config-ul
